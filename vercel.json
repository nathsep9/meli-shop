{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/favicon.svg",
      "dest": "/public/favicon.svg"
    },
    {
      "src": "/logo.png",
      "dest": "/public/logo.png"
    },
    {
      "src": "/not-found.svg",
      "dest": "/public/not-found.svg"
    },
    {
      "src": "/robots.txt",
      "dest": "/dist/server.js"
    },
    {
      "src": "/sitemap.xml",
      "dest": "/dist/server.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/server.js"
    },
    {
      "src": "/product/(.*)",
      "dest": "/dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ],
  "env": {
    "FAKESTORE_API": "https://fakestoreapi.com/products",
    "BASE_URL": "https://meli-shop-flouo6y5d-naliaab1809s-projects.vercel.app"
  }
}
